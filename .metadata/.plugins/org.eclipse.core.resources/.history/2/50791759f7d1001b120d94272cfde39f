package mycodes;

public class CDcube {

	public static long findNb(long m) {
		int sum = 0;
		int i = 0;
		long count =0;
		double n = Math.cbrt(m);
		if ((n) == Math.cbrt(m) ) return 1;
		else {
		}
		
		while (sum<(int)n)
		{
			sum += ((int)n - i)^3;
			i++;
			count ++;
		}
		
		return count ; 
		
	}	
	public static void main(String[] args) {
		System.out.println(findNb(135440716410000L));

	}
	
	//n^3 + (n-1)^3 + ... + 1^3 = m if such a n exists or -1 if there is no such n.
}
