package mycodes;

public class CDoutlier {
	
	public static int outlier (int [] numbers)
	{
		 
		int odd =0 ; 
		int even = 0 ; 
		for (int i =0; i<numbers.length;i++)
		{
			if (numbers[i] % 2 == 0 ) even ++ ; 
			if (numbers[i] % 2 != 0 ) odd ++ ; 
			
		}
		
		if ( odd == 1 ) return odd ;
		else return even ; 
		
		
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] exampleTest1 = {206847684,1056521,7,17,1901,21104421,7,1,35521,1,7781}; 
		System.out.println(outlier(exampleTest1));
	}

}
