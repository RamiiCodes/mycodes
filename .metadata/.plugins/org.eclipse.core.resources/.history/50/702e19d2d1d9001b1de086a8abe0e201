package mycodes;

public class CDCamelCase {
	static String toCamelCase(String s){
		String [] str = s.split("-");
	    return "";
	}
	public static void main(String[] args) {
		
		 String str= "abcdef";

	        String[] array=str.split("(?<=\\G.{2})");

	        System.out.println(java.util.Arrays.toString(array));
		

	}

}
