package mycodes;

import java.util.regex.Matcher;
import java.util.regex.Pattern;

public class CDCamelCase {
	static public String splt2 (String str)
	{
		String [] tab = str.split("_");
		return tab.toString();
		 
	}
	
	static public String splt (String str)
	{
		String [] tab = str.split("[-_]");
		return tab.toString();
		 
	}
	static String toCamelCase(String s){
		
		String [] str = s.split("[-_]");
		for (int i =0; i<str.length;i++)
		{System.out.println();}
		return str.toString();
	}
	public static void main(String[] args) {
		
		toCamelCase("the_Stealth-Warrior");
	

	}

}
