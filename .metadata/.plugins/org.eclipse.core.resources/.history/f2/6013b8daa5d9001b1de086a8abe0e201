package mycodes;

import java.util.Arrays;

public class CDDubstep {
	
	public static boolean isEqual (String str)
	{
		if (str.contains("WUB")) return true;
		return false ; 
	}
	
	 public static String SongDecoder (String song)
	  {
	    String [] arrSong = song.split("WUB");
	    Arrays.asList(arrSong).stream().filter(e -> e.toString().contentEquals("WUB")).r
	    
	   }

