/*Write a function that will return the count of distinct case-insensitive
 *  alphabetic characters and numeric digits that occur more than once in the input string. 
 *  The input string can be assumed to contain only alphabets (both uppercase and lowercase) 
 *  and numeric digits. */

package mycodes;

import java.util.*;
import java.util.stream.IntStream;



public class CDCountDuplicates {
	
	public static int digPow (int a , int b )
	
	{
		int sum = 0 ;
		String[] str = String.valueOf(a).split("");
		
		
		for (int i = 0; i < str.length ; i++) 
		{ 
		
		System.out.println(i);
		
			sum +=(int) Math.pow(parseInt(str[i]), i+1) ;
			System.out.println(sum);
			
		}
		
		System.out.println(sum);
		for (int i = b; i<= sum; i++)
		{
			if (sum / i == 0) return i;
		}
		return -1;
		
    }
		
		private static double parseInt(String string) {
		// TODO Auto-generated method stub
		return 0;
	}

		public static void main(String[] args) {
			System.out.println(digPow(89, 1));
			
		}


}
